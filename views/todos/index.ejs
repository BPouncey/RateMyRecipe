<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>First express</title>
  
</head>

<body>
  <h1> TODOS </h1>
  <ul>
    <% todos.forEach(function(t){ %>
      <li>
        <%= t.todo %>
        -
        <%= t.done ? 'done' :'not done' %>
      </li>
      <% }); %>
  </ul>
</body>

</html>